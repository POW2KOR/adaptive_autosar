<?xml version="1.0" ?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00047.xsd">
   <AR-PACKAGES>
      <AR-PACKAGE>
         <SHORT-NAME>mbition</SHORT-NAME>
         <ELEMENTS>
            <ETHERNET-CLUSTER>
               <SHORT-NAME>mbition-example-ethernet-cluster</SHORT-NAME>
               <ETHERNET-CLUSTER-VARIANTS>
                  <ETHERNET-CLUSTER-CONDITIONAL>
                     <PHYSICAL-CHANNELS>
                        <ETHERNET-PHYSICAL-CHANNEL>
                           <SHORT-NAME>mbition-example-physical-channel</SHORT-NAME>
                           <NETWORK-ENDPOINTS>
                              <NETWORK-ENDPOINT>
                                 <SHORT-NAME>mbition-example-network-endpoint</SHORT-NAME>
                                 <NETWORK-ENDPOINT-ADDRESSES>
                                    <IPV-4-CONFIGURATION>
                                       <IPV-4-ADDRESS>10.20.1.97</IPV-4-ADDRESS>
                                       <IPV-4-ADDRESS-SOURCE>FIXED</IPV-4-ADDRESS-SOURCE>
                                       <TTL>5</TTL>
                                    </IPV-4-CONFIGURATION>
                                 </NETWORK-ENDPOINT-ADDRESSES>
                              </NETWORK-ENDPOINT>
                              <NETWORK-ENDPOINT>
                                 <SHORT-NAME>mbition-example-sd-network-endpoint</SHORT-NAME>
                                 <NETWORK-ENDPOINT-ADDRESSES>
                                    <IPV-4-CONFIGURATION>
                                       <IPV-4-ADDRESS>239.20.0.2</IPV-4-ADDRESS>
                                    </IPV-4-CONFIGURATION>
                                 </NETWORK-ENDPOINT-ADDRESSES>
                              </NETWORK-ENDPOINT>
                           </NETWORK-ENDPOINTS>
                        </ETHERNET-PHYSICAL-CHANNEL>
                     </PHYSICAL-CHANNELS>
                  </ETHERNET-CLUSTER-CONDITIONAL>
               </ETHERNET-CLUSTER-VARIANTS>
            </ETHERNET-CLUSTER>
            <MACHINE-DESIGN>
               <SHORT-NAME>machine-design</SHORT-NAME>
               <COMMUNICATION-CONNECTORS>
                  <ETHERNET-COMMUNICATION-CONNECTOR>
                     <SHORT-NAME>mbition-example-ethernet-connector</SHORT-NAME>
                     <UNICAST-NETWORK-ENDPOINT-REF DEST="NETWORK-ENDPOINT">/mbition/mbition-example-ethernet-cluster/mbition-example-physical-channel/mbition-example-network-endpoint</UNICAST-NETWORK-ENDPOINT-REF>
                  </ETHERNET-COMMUNICATION-CONNECTOR>
               </COMMUNICATION-CONNECTORS>
               <SERVICE-DISCOVER-CONFIGS>
                  <SOMEIP-SERVICE-DISCOVERY>
                     <MULTICAST-SD-IP-ADDRESS-REF DEST="NETWORK-ENDPOINT">/mbition/mbition-example-ethernet-cluster/mbition-example-physical-channel/mbition-example-sd-network-endpoint</MULTICAST-SD-IP-ADDRESS-REF>
                     <SOMEIP-SERVICE-DISCOVERY-PORT>30490</SOMEIP-SERVICE-DISCOVERY-PORT>
                  </SOMEIP-SERVICE-DISCOVERY>
               </SERVICE-DISCOVER-CONFIGS>
            </MACHINE-DESIGN>
            <MODE-DECLARATION-GROUP>
               <SHORT-NAME>MachineStates</SHORT-NAME>
               <INITIAL-MODE-REF DEST="MODE-DECLARATION">/mbition/MachineStates/Startup</INITIAL-MODE-REF>
               <MODE-DECLARATIONS>
                  <MODE-DECLARATION>
                     <SHORT-NAME>Running</SHORT-NAME>
                     <VALUE>0</VALUE>
                  </MODE-DECLARATION>
                  <MODE-DECLARATION>
                     <SHORT-NAME>Startup</SHORT-NAME>
                     <VALUE>1</VALUE>
                  </MODE-DECLARATION>
                  <MODE-DECLARATION>
                     <SHORT-NAME>Shutdown</SHORT-NAME>
                     <VALUE>2</VALUE>
                  </MODE-DECLARATION>
                  <MODE-DECLARATION>
                     <SHORT-NAME>Restart</SHORT-NAME>
                     <VALUE>3</VALUE>
                  </MODE-DECLARATION>
                  <MODE-DECLARATION>
                     <SHORT-NAME>Off</SHORT-NAME>
                     <VALUE>4</VALUE>
                  </MODE-DECLARATION>
               </MODE-DECLARATIONS>
            </MODE-DECLARATION-GROUP>
            <MODE-DECLARATION-GROUP>
               <SHORT-NAME>ApplicationStates</SHORT-NAME>
               <INITIAL-MODE-REF DEST="MODE-DECLARATION">/mbition/ApplicationStates/Terminating</INITIAL-MODE-REF>
               <MODE-DECLARATIONS>
                  <MODE-DECLARATION>
                     <SHORT-NAME>Running</SHORT-NAME>
                     <VALUE>0</VALUE>
                  </MODE-DECLARATION>
                  <MODE-DECLARATION>
                     <SHORT-NAME>Terminating</SHORT-NAME>
                     <VALUE>1</VALUE>
                  </MODE-DECLARATION>
               </MODE-DECLARATIONS>
            </MODE-DECLARATION-GROUP>
            <MACHINE>
               <SHORT-NAME>example-machine</SHORT-NAME>
               <FUNCTION-GROUPS>
                  <MODE-DECLARATION-GROUP-PROTOTYPE>
                     <SHORT-NAME>application-mode-declaration-group-prototype</SHORT-NAME>
                     <TYPE-TREF DEST="MODE-DECLARATION-GROUP">/mbition/ApplicationStates</TYPE-TREF>
                  </MODE-DECLARATION-GROUP-PROTOTYPE>
                  <MODE-DECLARATION-GROUP-PROTOTYPE>
                     <SHORT-NAME>MachineState</SHORT-NAME>
                     <TYPE-TREF DEST="MODE-DECLARATION-GROUP">/mbition/MachineStates</TYPE-TREF>
                  </MODE-DECLARATION-GROUP-PROTOTYPE>
               </FUNCTION-GROUPS>
               <MACHINE-DESIGN-REF DEST="MACHINE-DESIGN">/mbition/machine-design</MACHINE-DESIGN-REF>
               <MODULE-INSTANTIATIONS>
                  <OS-MODULE-INSTANTIATION>
                     <SHORT-NAME>mbition-os-mod-instantiation</SHORT-NAME>
                  </OS-MODULE-INSTANTIATION>
                  <GENERIC-MODULE-INSTANTIATION>
                     <SHORT-NAME>generic-mod-instantiation</SHORT-NAME>
                     <ADMIN-DATA>
                        <SDGS>
                           <SDG GID="DV:Adaptive">
                              <SDG GID="DV:Updatemanager">
                                 <SD GID="DV:PathToApplications">/opt</SD>
                                 <SD GID="DV:ImageDownloadPath">/mnt/fs-swap</SD>
                              </SDG>
                           </SDG>
                        </SDGS>
                     </ADMIN-DATA>
                  </GENERIC-MODULE-INSTANTIATION>
               </MODULE-INSTANTIATIONS>
               <PROCESSORS>
                  <PROCESSOR>
                     <SHORT-NAME>processor</SHORT-NAME>
                     <CORES>
                        <PROCESSOR-CORE>
                           <SHORT-NAME>core</SHORT-NAME>
                           <ADMIN-DATA>
                              <SDGS>
                                 <SDG GID="DV:Adaptive">
                                    <SD GID="DV:ProcessorCoreNumber">0</SD>
                                 </SDG>
                              </SDGS>
                           </ADMIN-DATA>
                        </PROCESSOR-CORE>
                     </CORES>
                  </PROCESSOR>
               </PROCESSORS>
            </MACHINE>
         </ELEMENTS>
      </AR-PACKAGE>
   </AR-PACKAGES>
</AUTOSAR>
