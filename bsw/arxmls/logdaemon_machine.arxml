<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00047.xsd">
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>vector</SHORT-NAME>
      <ELEMENTS>
        <MACHINE>
          <SHORT-NAME>LogMachine</SHORT-NAME>
          <ADMIN-DATA>
            <SDGS>
              <SDG GID="DvMex:MachineExtensions">
                <SDG GID="DvMex:EcuId">
                  <SD>ECU1</SD>
                </SDG>
              </SDG>
            </SDGS>
          </ADMIN-DATA>
          <FUNCTION-GROUPS>
            <MODE-DECLARATION-GROUP-PROTOTYPE>
              <SHORT-NAME>MachineState</SHORT-NAME>
              <TYPE-TREF DEST="MODE-DECLARATION-GROUP">/vector/MachineStates</TYPE-TREF>
            </MODE-DECLARATION-GROUP-PROTOTYPE>
          </FUNCTION-GROUPS>
          <MACHINE-DESIGN-REF DEST="MACHINE-DESIGN">/vector/LogMachineDesign</MACHINE-DESIGN-REF>
          <MODULE-INSTANTIATIONS>
            <LOG-AND-TRACE-INSTANTIATION>
              <SHORT-NAME>LogAndTraceInstantiation</SHORT-NAME>
              <ADMIN-DATA>
                <SDGS>
                  <SDG GID="DvMex:LogAndTraceInstantiationExtensions">
                    <SDG GID="DvMex:IpcPort">
                      <SD>33</SD>
                    </SDG>
                    <SDG GID="DvMex:OutputConfiguration">
                      <SDG GID="DvMex:OutputConfigurationClass">
                        <SDG-CAPTION>
                          <SHORT-NAME>tcp0</SHORT-NAME>
                        </SDG-CAPTION>
                        <SDG GID="DvMex:MessageBufferSize">
                          <SD>1000</SD>
                        </SDG>
                        <SDG GID="DvMex:BufferStrategy">
                          <SD>DROP-NEW</SD>
                        </SDG>
                        <SDG GID="DvMex:NetworkConfiguration">
                          <SDX-REF DEST="ETHERNET-NETWORK-CONFIGURATION">/vector/LogMachine/LogAndTraceInstantiation/EthernetNetworkConfiguration</SDX-REF>
                        </SDG>
                      </SDG>
                    </SDG>
                  </SDG>
                </SDGS>
              </ADMIN-DATA>
              <NETWORK-CONFIGURATIONS>
                <ETHERNET-NETWORK-CONFIGURATION>
                  <SHORT-NAME>EthernetNetworkConfiguration</SHORT-NAME>
                  <COMMUNICATION-CONNECTOR-REF DEST="ETHERNET-COMMUNICATION-CONNECTOR">/vector/LogMachineDesign/EthernetConnector</COMMUNICATION-CONNECTOR-REF>
                  <TCP-PORT>49361</TCP-PORT>
                </ETHERNET-NETWORK-CONFIGURATION>
              </NETWORK-CONFIGURATIONS>
            </LOG-AND-TRACE-INSTANTIATION>
          </MODULE-INSTANTIATIONS>
        </MACHINE>
        <MACHINE-DESIGN>
          <SHORT-NAME>LogMachineDesign</SHORT-NAME>
          <COMMUNICATION-CONNECTORS>
            <ETHERNET-COMMUNICATION-CONNECTOR>
              <SHORT-NAME>EthernetConnector</SHORT-NAME>
              <ADMIN-DATA>
                <SDGS>
                  <SDG GID="DvMex:EthernetCommunicationConnectorExtensions">
                    <SDG GID="DvMex:KeepAlive">
                      <SD>true</SD>
                    </SDG>
                    <SDG GID="DvMex:NoDelay">
                      <SD>false</SD>
                    </SDG>
                    <SDG GID="DvMex:NumConnections">
                      <SD>1</SD>
                    </SDG>
                  </SDG>
                </SDGS>
              </ADMIN-DATA>
              <UNICAST-NETWORK-ENDPOINT-REF DEST="NETWORK-ENDPOINT">/vector/Ethernet/EthernetChannel1/NetworkEndpointLogd</UNICAST-NETWORK-ENDPOINT-REF>
            </ETHERNET-COMMUNICATION-CONNECTOR>
          </COMMUNICATION-CONNECTORS>
        </MACHINE-DESIGN>
        <ETHERNET-CLUSTER>
          <SHORT-NAME>Ethernet</SHORT-NAME>
          <ETHERNET-CLUSTER-VARIANTS>
            <ETHERNET-CLUSTER-CONDITIONAL>
              <PHYSICAL-CHANNELS>
                <ETHERNET-PHYSICAL-CHANNEL>
                  <SHORT-NAME>EthernetChannel1</SHORT-NAME>
                  <NETWORK-ENDPOINTS>
                    <NETWORK-ENDPOINT>
                      <SHORT-NAME>NetworkEndpoint</SHORT-NAME>
                      <NETWORK-ENDPOINT-ADDRESSES>
                        <IPV-4-CONFIGURATION>
                          <IPV-4-ADDRESS>192.168.7.2</IPV-4-ADDRESS>
                          <IPV-4-ADDRESS-SOURCE>FIXED</IPV-4-ADDRESS-SOURCE>
                          <TTL>5</TTL>
                        </IPV-4-CONFIGURATION>
                      </NETWORK-ENDPOINT-ADDRESSES>
                    </NETWORK-ENDPOINT>
                    <NETWORK-ENDPOINT>
                      <SHORT-NAME>NetworkEndpointLogd</SHORT-NAME>
                      <NETWORK-ENDPOINT-ADDRESSES>
                        <IPV-4-CONFIGURATION>
                          <IPV-4-ADDRESS>10.21.17.98</IPV-4-ADDRESS>
                          <IPV-4-ADDRESS-SOURCE>FIXED</IPV-4-ADDRESS-SOURCE>
                          <TTL>5</TTL>
                        </IPV-4-CONFIGURATION>
                      </NETWORK-ENDPOINT-ADDRESSES>
                    </NETWORK-ENDPOINT>
                    <NETWORK-ENDPOINT>
                      <SHORT-NAME>SdMulticastEndpoint</SHORT-NAME>
                      <NETWORK-ENDPOINT-ADDRESSES>
                        <IPV-4-CONFIGURATION>
                          <IPV-4-ADDRESS>224.0.0.17</IPV-4-ADDRESS>
                        </IPV-4-CONFIGURATION>
                      </NETWORK-ENDPOINT-ADDRESSES>
                    </NETWORK-ENDPOINT>
                  </NETWORK-ENDPOINTS>
                </ETHERNET-PHYSICAL-CHANNEL>
              </PHYSICAL-CHANNELS>
            </ETHERNET-CLUSTER-CONDITIONAL>
          </ETHERNET-CLUSTER-VARIANTS>
        </ETHERNET-CLUSTER>
        <MODE-DECLARATION-GROUP>
          <SHORT-NAME>MachineStates</SHORT-NAME>
          <INITIAL-MODE-REF DEST="MODE-DECLARATION">/vector/MachineStates/Running</INITIAL-MODE-REF>
          <MODE-DECLARATIONS>
            <MODE-DECLARATION>
              <SHORT-NAME>Running</SHORT-NAME>
              <VALUE>0</VALUE>
            </MODE-DECLARATION>
            <MODE-DECLARATION>
              <SHORT-NAME>Startup</SHORT-NAME>
              <VALUE>1</VALUE>
            </MODE-DECLARATION>
            <MODE-DECLARATION>
              <SHORT-NAME>Shutdown</SHORT-NAME>
              <VALUE>2</VALUE>
            </MODE-DECLARATION>
            <MODE-DECLARATION>
              <SHORT-NAME>Restart</SHORT-NAME>
              <VALUE>3</VALUE>
            </MODE-DECLARATION>
            <MODE-DECLARATION>
              <SHORT-NAME>Off</SHORT-NAME>
              <VALUE>4</VALUE>
            </MODE-DECLARATION>
          </MODE-DECLARATIONS>
        </MODE-DECLARATION-GROUP>
        <MODE-DECLARATION-GROUP>
          <SHORT-NAME>ApplicationStates</SHORT-NAME>
          <MODE-DECLARATIONS>
            <MODE-DECLARATION>
              <SHORT-NAME>Running</SHORT-NAME>
              <VALUE>0</VALUE>
            </MODE-DECLARATION>
            <MODE-DECLARATION>
              <SHORT-NAME>Terminating</SHORT-NAME>
              <VALUE>1</VALUE>
            </MODE-DECLARATION>
          </MODE-DECLARATIONS>
        </MODE-DECLARATION-GROUP>
      </ELEMENTS>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>
